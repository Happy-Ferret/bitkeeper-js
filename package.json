{
  "name": "bitkeeper-js",
  "version": "0.0.1",
  "description": "Tradle bitkeeper component in JS",
  "main": "index.js",
  "scripts": {
    "jsbeautify": "js-beautify -r {*.js,lib/*.js,test/*.js} --config node_modules/tradle-style/.jsbeautifyrc",
    "jshint": "jshint {*.js,**/*.js} --config node_modules/tradle-style/.jshintrc",
    "jscs": "jscs {*.js,lib/*.js,test/*.js} --config node_modules/tradle-style/.jscsrc",
    "test": "node test/*.js"
  },
  "pre-commit": [
    "test",
    "jscs",
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/bitkeeper-js.git"
  },
  "bugs": {
    "url": "https://github.com/tradle/bitkeeper-js/issues"
  },
  "homepage": "https://github.com/tradle/bitkeeper-js",
  "keywords": [
    "bitcoin",
    "blockchain",
    "tradenet",
    "tradle",
    "distributed",
    "storage",
    "decentralized",
    "webtorrent",
    "storage"
  ],
  "browser": {
    "promise-ports": false,
    "net": "chrome-net",
    "dgram": "chrome-dgram",
    "fs": "browserify-fs",
    "dns": "dns.js"
  },
  "license": "MIT",
  "dependencies": {
    "bittorrent-dht": "git://github.com/mvayngrib/bittorrent-dht.git#5f53c9",
    "body-parser": "^1.10.0",
    "browserify-fs": "^1.0.0",
    "bs58": "^2.0.1",
    "chrome-dgram": "^2.1.0",
    "chrome-net": "^2.4.0",
    "concat-stream": "^1.4.7",
    "create-torrent": "^3.2.1",
    "debug": "^2.1.0",
    "express": "^4.10.6",
    "mkdirp": "^0.5.0",
    "nedb": "^1.0.1",
    "parse-torrent": "^2.3.0",
    "promise-ports": "git://github.com/tradle/promise-ports.git#bf985c",
    "q": "^1.1.2",
    "re-emitter": "^1.0.0",
    "rimraf": "^2.2.8",
    "simple-get": "^1.3.0",
    "simple-jobs": "^0.0.1",
    "tradle-utils": "git://github.com/tradle/tradle-utils.git#590eef",
    "walk": "^2.3.5",
    "webtorrent": "git://github.com/tradle/webtorrent#e6c5bd"
  },
  "devDependencies": {
    "js-beautify": "^1.5.4",
    "jscs": "^1.10.0",
    "jshint": "latest",
    "minimist": "^1.1.0",
    "pre-commit": "0.0.11",
    "tape": "^3.2.0",
    "tradle-style": "latest"
  }
}
